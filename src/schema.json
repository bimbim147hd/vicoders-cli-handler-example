{
  "$schema": "http://json-schema.org/schema",
  "id": "VicodersCliExampleHandler",
  "title": "Vicoders CLI example handler",
  "type": "object",
  "description": "Example handler",
  "properties": {
    "path": {
      "type": "string",
      "description": "The root path of project",
      "default": "/"
    },
    "plugin": {
      "description": "Which plugin do you want to install?",
      "type": "string",
      "default": "css",
      "enum": ["regenerate-thumbnails", "classic-editor"],
      "x-prompt": "Which plugin do you want to install?"
    }
  }
}
